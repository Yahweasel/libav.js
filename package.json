{
  "name": "@descript/libav.js",
  "version": "4.3.6",
  "description": "A compilation of the libraries associated with handling audio and video in ffmpeg—libavformat, libavcodec, libavfilter, libavutil and libswresample—for WebAssembly and asm.js, and thus the web.",
  "main": "dist/libav-4.3.6-descript-p3.js",
  "types": "dist/libav.types.d.ts",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "make build-all -j9",
    "test": "npm run build && cd tests && node node-test.js --include-slow",
    "superclean": "git clean -f -d -x",
    "version-bump": "version-bump"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/descript/libav.js.git"
  },
  "keywords": [
    "ffmpeg",
    "libav",
    "audio",
    "video",
    "av",
    "encode",
    "decode",
    "transcode",
    "mux",
    "demux"
  ],
  "files": [
    "dist/*"
  ],
  "author": "descript",
  "license": "LGPL-2.1",
  "bugs": {
    "url": "https://github.com/descript/libav.js/issues"
  },
  "homepage": "https://github.com/descript/libav.js#readme",
  "devDependencies": {
    "uglify-js": "^3.14.3"
  },
  "dependencies": {
    "fast-xml-parser": "^4.2.2"
  }
}
